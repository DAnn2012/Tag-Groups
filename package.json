{
  "name": "chatty-mango-guten-tag",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "wp-scripts build",
    "start": "wp-scripts start",
    "// build:css": "css-minify -d ./assets/css -o ./assets/css",
    "build:scss": "node-sass --output-style expanded ./assets/scss -o ./assets/css",
    "watch:scss": "npm run build:scss && node-sass --output-style expanded --watch ./assets/scss -o ./assets/css"
  },
  "dependencies": {
    "@wordpress/api-fetch": "^2.0.0",
    "css-minify": "^1.1.0",
    "node-sass": "^5.0.0",
    "react-select": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@wordpress/scripts": "^12.2.0",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-replace": "^1.1.3",
    "gulp-zip": "^5.1.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
